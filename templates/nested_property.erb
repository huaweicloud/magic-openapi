<%
  data_type = Hash[
    "string" => "- !ruby/object:Api::Type::String",
    "boolean" => "- !ruby/object:Api::Type::Boolean",
    "integer" => "- !ruby/object:Api::Type::Integer",
  ]
-%>
	properties:
<% props.each do |key, value| -%>
          <%= data_type[value['type']] %>
          name: '<%= key %>'
          description: '<%= value['description'] %>'
<% end -%>
